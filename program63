/*
write a program to take the size of the array,
accept elements of the array, and accept an element
print how many times the given element is present in an array 
*/
#include<stdio.h>
int main()
{
	int size;
	scanf("%d",&size);
	int arr[size];
	int i;
	for(i=0;i<size;i++)
	{
		scanf("%d",&arr[i]);
	}
	int element;
	scanf("%d",&element);
	int c = 0;
	for(i=0;i<size;i++)
	{
		if(element == arr[i])
		{
			c++;
		}
	}
	printf("%d",c);
	return 0;
}
